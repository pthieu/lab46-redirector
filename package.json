{
  "name": "lab46-redirector",
  "version": "1.0.0",
  "description": "URL shortener for LAB46",
  "author": "Phong Thieu",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "vite-node --watch src/*.ts",
    "build": "tsc && node esbuild.mjs",
    "serve": "node dist/app.js",
    "format": "pnpm run lint --fix & prettier --write 'src/**/*.{js,ts}'",
    "lint:fix": "eslint src --ext .ts --fix",
    "lint": "eslint src --ext .ts",
    "migrate:create": "drizzle-kit generate:pg --config=drizzle.config.ts --custom",
    "migrate:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrate:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "migrate:check": "drizzle-kit check:pg --config=drizzle.config.ts"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/is-url": "^1.2.30",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "drizzle-kit": "^0.18.1",
    "esbuild": "^0.17.19",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-node": "^0.31.2",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.26.3",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "is-url": "^1.2.4",
    "nanoid": "^4.0.2",
    "pg": "^8.11.0"
  }
}
